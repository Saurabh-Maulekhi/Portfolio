{
  "builds": [{
    "src" : "Portfolio/wsgi.py",
    "use" : "@vercel/python",
  },
  {
    "src": "staticfiles/**",
    "use": "@vercel/static"
  }
  ],

  "routes": [
    {
      "src": "/static/(.)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Portfolio/wsgi.py"
    }
  ]
}
