{
  "builds": [{
    "src" : "Portfolio/wsgi.py",
    "use" : "@vercel/python",
    "config" : { "maxLambdaSize": "15mb", "runserver": "python3.9" }
  }],

  "routes": [
    {
      "src": "/static/(.)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Portfolio/wsgi.py"
    }
  ]
}
